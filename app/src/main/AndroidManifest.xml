        android:supportsRtl="true"
        android:theme="@style/Theme.Naruto">
        <activity android:name=".Splash" android:theme="@style/Theme.SemBara">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".PersonagensAkatsuki" android:theme="@style/Theme.SemBara" />
        <activity android:name=".NinjasAkatsuki" android:theme="@style/Theme.SemBara" />
        <activity android:name=".MenuInicial" android:theme="@style/Theme.SemBara" />
        <activity android:name=".PersonagensKonoha" android:theme="@style/Theme.SemBara" />
        <activity android:name=".NinjasKonoha" android:theme="@style/Theme.SemBara"></activity>
    </application>

</manifest>